extends layout

block append head
    script(src="/javascript/house/houseList.js")
    link(rel="stylesheet", type="text/css", href="/css/house/housePost.css")

block content
    each house in houses
        a(href="/house/view/#{house._id}") #{house.title}
        span |
        a(href="/house/delete/#{house._id}") del
        br

    div.popin
        div#map